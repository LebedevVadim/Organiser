@model Dictionary<Organiser.Components.Week, IEnumerable<IEvent>>

@if (Model.Count() > 0)
{
    @foreach (var key in Model.Keys)
    { 
        <h2>@key.BeginWeek.ToString("d") - @key.EndWeek.ToString("d")</h2>
        @await Html.PartialAsync("ListEvents", Model[key]);
    }
}
else
{
    <div>
        Записей нет.
    </div>
}