@model Meet

<form asp-action="EditMeet" method="post">
    @await Html.PartialAsync("EventField", Model)
    <div class="form-group">
        <label asp-for="EndDate"></label>
        <div><span asp-validation-for="EndDate" class="text-danger"></span></div>
        <input type="datetime-local" asp-for="EndDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Place"></label>
        <div><span asp-validation-for="Place" class="text-danger"></span></div>
        <input asp-for="Place" class="form-control" />
    </div>
    <div class="text-center">
        <button class="btn btn-primary" type="submit">Save</button>
        <a asp-action="List" class="btn">Cancel</a>
    </div>
</form>

